    "cypress-file-upload": "^5.0.8",
    "fetch-intercept": "^2.4.0",
    "formik": "^2.2.9",
    "history": "^5.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.30.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^5.0.0",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.3.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@faker-js/faker": "^6.1.2",
    "@storybook/addon-a11y": "^6.4.22",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/node-logger": "^6.4.22",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.29",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.1.2",
    "@types/styled-components": "^5.1.25",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitlint": "^16.2.1",
    "cypress": "^9.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^7.0.4",
    "jest": "^28.0.3",
    "jest-styled-components": "^7.0.8",
    "jest-watch-typeahead": "^0.6.5",
    "lint-staged": "^12.3.4",
    "msw": "^0.39.2",
    "prettier": "^2.5.1",
    "redux-saga-test-plan": "^4.0.4",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "stylelint": "^14.5.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.9",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.3",
    "validate-branch-name": "^1.3.0",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "scripts": {
    "pre:start": "node ./scripts/update-modules-alias.js",
    "start": "npm run pre:start cross-env NODE_ENV=development && npx husky install && craco start",
    "build": "npm run pre:start cross-env NODE_ENV=production && GENERATE_SOURCEMAP=false craco build",
    "test": "npm run pre:start && craco test",
    "prettier": "prettier --write \"./**/*.js\"",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "lint:branch": "validate-branch-name",
    "sb": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "validate-branch-name": {
    "pattern": "^(master|main|dev|qa){1}$|^(feature|fix|hotfix|release)/.+$",
    "errorMsg": "Invalid branch name. Must be one of: master, main, develop, feature/<name>, fix/<name>, hotfix/<name>, release/<name>"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended"
    ],
    "rules": {
      "react/jsx-key": "error"
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test -- --watchAll=false",
      "pre-commit": "validate-branch-name && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "stylelint --fix"
    ]
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=14.0.0"
  },
  "author": {
    "name": "Islom Toshpulatov",
    "email": "nanotexnolagiya@gmail.com",
    "url": "https://linkedin.com/in/nanotexnolagiya",
    "username": "nanotexnolagiya"
  }
}
